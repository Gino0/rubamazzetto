        xmlns:local="clr-namespace:ProgettoTecnologie"
        mc:Ignorable="d"
        Title="FinestraGuardaInviti" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="sfondotavologioco.jpeg"/>
        </Grid.Background>
        <Button x:Name="btnBack" Content="TURN BACK" HorizontalAlignment="Left" Margin="10,340,0,0" VerticalAlignment="Top" Width="156" Height="69" Click="btnBack_Click"/>
        <Label x:Name="lblInvito" Content="Nessun invito ricevuto" HorizontalAlignment="Left" Margin="74,50,0,0" VerticalAlignment="Top" Width="373" Height="36" FontSize="20"/>
        <Button x:Name="btnAccetta" Content="ACCETTA" HorizontalAlignment="Left" Margin="566,50,0,0" VerticalAlignment="Top" Width="153" Height="36" IsEnabled="False"/>

    </Grid>
</Window>
